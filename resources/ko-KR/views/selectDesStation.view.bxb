result-view{
  match: ToSelectedResult(result)
  
  message(도착역을 선택해주세요)
  
  render{
    layout{
      section{
        content{
          for-each (result.desList){
            as(desList){
              layout-macro (select_des_station){
                param (toStation){
                  expression (toStation)
                }
                param (desResult){
                  expression (desList)
                }
              }
            }
          }
        }
      }
    }
  }
}