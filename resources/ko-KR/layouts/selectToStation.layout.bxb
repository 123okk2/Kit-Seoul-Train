layout-macro-def (select_to_station) {
  params {
    param (toResult){
      type(ToResult)
      min (Required)
      max (One)
    }
    
    param (desResult){
      type(DesResult)
      min (Required)
      max (Many)
    }
  }

  content{
    cell-card{
      on-click{
        intent{
          goal: ToSelectedResult
          value: ToSelectedResult{
            toStation: ToStation( toResult )
            desList: DesResult( desResult )
          }
        }
      }
      slot2{
        content{
          order (PrimarySecondary)
          primary("#{value(toResult)}")
        }
      }
    }
  }
}
