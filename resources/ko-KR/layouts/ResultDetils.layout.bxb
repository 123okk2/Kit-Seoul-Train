layout-macro-def (result_details) {
  params {
    param (info) {
      type (StationResult)
      min (Required) max (One)
    }
  }
  
  content {
    cell-card {
      slot2 {
          content {
            order (PrimarySecondary)
          primary ("#{value(info.toList)}")
          }
        }
     }
  }
}