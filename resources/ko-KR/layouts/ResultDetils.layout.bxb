layout-macro-def (result_details) {
  params {
    param (info) {
      type (Result)
      min (Required) max (One)
    }
  }
  
  content {
    cell-card {
      slot2 {
          content {
            order (PrimarySecondary)
          primary ("#{value(info.to)} 에서 #{value(info.des)} 까지는 #{value(info.result)}분 소요됩니다.")
          }
        }
     }
  }
}