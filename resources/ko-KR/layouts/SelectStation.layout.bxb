layout-macro-def (select_station) {
  params {
    param (stationResult){
      type(StationResult)
      min (Required)
      max (One)
    }
  }

  content{
    cell-card{
      on-click{
        intent{
          goal: StationResult
        }
      }
      slot2{
        content{
          order (PrimarySecondary)
          primary("#{value(stationResult.toList)}")
        }
      }
    }
  }
}
